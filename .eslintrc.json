{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "parser": "babel-eslint",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "quotes": [
            "error",
            "single"
        ],
        "comma-dangle": 0,
        "no-unused-vars": [
            "error",
            {
                "varsIgnorePattern": "React"
            }
        ],
        "linebreak-style": 0,
        "max-len": [
            "error",
            {
                "code": 130,
                "ignoreUrls": true,
                "ignoreStrings": true,
                "ignoreTemplateLiterals": true
            }
        ],
        "react/prop-types": 0,
        "no-return-assign": 0,
        "no-param-reassign": 0,
        "object-curly-newline": 0,
        "import/no-unresolved": 0,
        "import/extensions": 0,
        "consistent-return": 0,
        "func-names": 0,
        "no-dupe-class-members": 0,
        "import/no-cycle": 0,
        "react/state-in-constructor": 0,
        "react/no-unused-state": 0,
        "class-methods-use-this": 0,
        "array-callback-return": 0,
        "no-plusplus": 0,
        "no-shadow": 0,
        "camelcase": 0,
        "eqeqeq": 0,
        "react/sort-comp": 0,
        "prefer-rest-params": 0,
        "default-param-last": 0,
        "react/jsx-props-no-spreading": 0,
        "react/destructuring-assignment": 0,
        "react/no-access-state-in-setstate": 0,
        "react/function-component-definition": 0
    },
    "plugins": [
        "react"
    ],
    "settings": {
        "react": {
            "version": "detect"
        },
        "import/resolver": {
            "node": {
                "extensions": [
                    ".js",
                    ".jsx",
                    ".ts",
                    ".tsx"
                ]
            }
        }
    }
}